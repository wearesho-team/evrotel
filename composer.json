{
  "name": "wearesho-team/evrotel",
  "description": "Evrotel API Integration",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Letnikow",
      "email": "reclamme@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2",
    "horat1us/environment-config": "^1.2",
    "guzzlehttp/guzzle": "^6.3",
    "nesbot/carbon": "^1.30",
    "wearesho-team/base-collection": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "scripts": {
    "lint": "./vendor/bin/phpcs --standard=PSR2 ./src ./tests",
    "phpcbf": "./vendor/bin/phpcbf --standard=PSR2 ./src ./tests",
    "test": "./vendor/bin/phpunit"
  },
  "autoload": {
    "psr-4": {
      "Wearesho\\Evrotel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wearesho\\Evrotel\\Tests\\": "tests/"
    }
  }
}
